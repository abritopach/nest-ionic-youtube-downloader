{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "headers": {"access-control-allow-origin": "*"},
            "methods": [
                "GET",
                "OPTIONS",
                "PATCH",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
